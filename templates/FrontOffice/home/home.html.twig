{% extends 'FrontOffice/base.html.twig' %}
{% block body %}
<section
        class="padding-y-150 flex-center jarallax"
        data-dark-overlay="5"
        style="background: url({{ asset('FrontOffice/assets/img/1920x800/5.jpg') }})"
>
    <div class="container">
        <div class="row">
            <div class="col-12 text-white text-center">
